@model List<Model>

<div>
     <h3>Models</h3>

     <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>YearOfRelease</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var fridgeModel in @Model)
            {
                <tr>
                    <td>@fridgeModel.Id</td>
                    <td>@fridgeModel.Name</td>
                    <td>@fridgeModel.YearOfRelease</td>
                </tr>
            }
        </tbody>
     </table>

     <a class="back-btn" href="Index">Back</a>
</div>